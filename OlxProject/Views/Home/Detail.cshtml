@model IEnumerable<OlxProject.Attribute>

@{ 
    var i = 0;
}

@foreach (var item in Model)
{

  

        <label style="margin-left:15px; width:100px;margin-top:10px;" >@item.Key</label>

        <input type="hidden" name="Attribute[@i].key" value="@item.Key" />

        <input type="text" name="Attribute[@i].value"  />
  

    i++;
    if (i % 1 == 0)
    {
        <br />
    }
}